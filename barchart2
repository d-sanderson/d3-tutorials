.chart rect {

    fill: steelblue
}

.chart text {
    fill:white;
    font: 10px sans-serif;
    text-anchor: end;
}
    
    var width = 420, 
    barHeight = 20;

    var x = d3.scale.linear()
        .range([0, width]);

    var chart = d3.select(".chart")
        .attr("width", width);
    d3.tsv("data.tsv", type, function(error, data) {
        x.domain([0, d3.max(data,function(d) { return d.value; })]);
        chart.attr("height", barHeight * data.length)
    });